{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false,
    "noImplicitThis": false
  }
}